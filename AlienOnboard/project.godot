;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="AlienOnboard"
run/main_scene="res://Scenes/BTTest.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="AlienOnBoardSaveDirectory"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/show_image=false
boot_splash/image="res://Textures/3.png"
config/icon="res://icon.svg"

[autoload]

ConfigFileHandler="*res://singletons/ConfigFileHandler.gd"
StateMachine="*res://Scripts/AlarmSystem/StateMachine/StateMachine.gd"
BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"
BeehaveGlobalDebugger="*res://addons/beehave/debug/global_debugger.gd"
EnemyManager="*res://Scripts/Enemy/EnemyManager.gd"

[display]

window/size/viewport_width=1600
window/size/viewport_height=1200
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="AlienOnboard"

[editor_plugins]

enabled=PackedStringArray("res://addons/beehave/plugin.cfg", "res://addons/phantom_camera/plugin.cfg")

